  <script type="text/javascript">
if (document.all){
window.attachEvent('onload',widfn);
}
else{
window.addEventListener('load', widfn,false);
}
function  widfn(){
(function(){
    var loginNcolse = document.getElementById("loginNcolse") ;
    var loginNwid = document.getElementById("loginNwid") ;
    $(document).click(function(e){
        if ($(e.target).closest("#loginNwid").length || $(e.target).closest("#person-service").length ){
            return;
        }
        loginNwid.style.display = 'none' ;
    });

    loginNcolse.onclick = function(){
        loginNwid.style.display = 'none' ;
    }
    ;$(".loginNFormInput").focus(function(){
        $(".errorstyle").hide();
    });
    if(document.all){
        $("#loginname").val("输入个人用户名/邮箱");
        $("#loginname").focus(function(){
            if($(this).val() == '输入个人用户名/邮箱'){$(this).val('');}
        }).blur(function(){
            if($(this).val() == ''){$(this).val('输入个人用户名/邮箱');}
        });
        function passwordfn(){
            if($("#password").val() == ''){
                if($("#passwordtxt").length<1){$("#loginNwid").append('<div id="passwordtxt">输入密码</div>');}
                $("#passwordtxt").show();
                $("#passwordtxt").focus(function(){
                    $("#passwordtxt").hide();
                    $("#password").focus();
                });
            }
        }
        passwordfn();
        $("#password").focus(function(){$("#passwordtxt").hide()}).blur(passwordfn);
    }

})();
    window.zlzpUtils = {};
    var personSer = $("#person-service") ;
    function GetCookie(name) {
        var arg = name + "=";
        var alen = arg.length;
        var clen = document.cookie.length;
        var i = 0;
        while (i < clen) {
            var j = i + alen;
            if (document.cookie.substring(i, j) == arg) return getCookieVal(j);
            i = document.cookie.indexOf(" ", i) + 1;
            if (i == 0) break;
        }
        return null;
    }
    function getCookieVal(offset) {
        var endstr = document.cookie.indexOf(";", offset);
        if (endstr == -1) endstr = document.cookie.length;
        return decodeURI(document.cookie.substring(offset, endstr));
    }
    var a = GetCookie("JSpUserInfo");
    var b = GetCookie("JSsUserInfo"); 
    function GetUserName(){
            var c = GetCookie("JSShowname");
            $("#companyLoginregin").hide();
            $("#companyLogining").show();
            $("#personname").text(c);
    }
    if(a!=null ||b!=null){GetUserName();}
    personSer.click(function(){
        recordOutboundLink(this,'addnewlink','personlogin');
        var a_ = GetCookie("JSpUserInfo");
        var b_ = GetCookie("JSsUserInfo");
        var c_ = GetCookie("JSShowname");       
        if(a_!=null ||b_!=null){GetUserName();}else{$("#loginNwid").show();$("#personname").text(c_);}
        
    });
    (function(i) {
        i.cookie = function(i, m, q) {
            if (1 < arguments.length && "[object Object]" !== "" + m) {
                q = jQuery.extend({},
                q);
                if (null === m || void 0 === m) q.expires = -1;
                if ("number" === typeof q.expires) {
                    var h = q.expires,
                    k = q.expires = new Date;
                    k.setDate(k.getDate() + h);
                }
                m = "" + m;
                return document.cookie = [encodeURIComponent(i), "=", q.raw ? m: encodeURIComponent(m), q.expires ? "; expires=" + q.expires.toUTCString() : "", q.path ? "; path=" + q.path: "", q.domain ? "; domain=" + q.domain: "", q.secure ? "; secure": ""].join("");
            }
            q = m || {};
            k = q.raw ?
            function(h) {
                return h
            }: decodeURIComponent;
            return (h = RegExp("(?:^|; )" + encodeURIComponent(i) + "=([^;]*)").exec(document.cookie)) ? k(h[1]) : null;
        }
    })(jQuery);
    zlzpUtils.logout = function() {
        var i = {
            path: "/",
            domain: ".zhaopin.com"
        };
        $.cookie("JSsUserInfo", null, i);
        $.cookie("JSNewlogin", null, i);
        $.cookie("JSpUserInfo", null, i);
        $.cookie("JSloginnamecookie", null, i);
        $.cookie("JSShowname", null, i);
        $.cookie("browsecount", null, i);
        $.cookie("postcount", null, i);
        $.cookie("mailcount", null, i);
        $.cookie("SJSsUserInfo", null, i);
        $.cookie("JSisLogin", null, i);
    };
    $("#personcheckout").click(function() {
        $("#companyLogining").hide();
        $("#companyLoginregin").show();
        $("#password").val("");
        zlzpUtils.logout();
    });
      
    var i_ = 1;
    $(".loginNFormBtn").click(function(){
        if($(".loginNformcb").attr("checked") == 'checked' ){ i_= 1}else{i_ = 0 ;}
        $.ajax({
            type: "post",
            url: "http://my.zhaopin.com/v5/handler/loginHandler.ashx",
            dataType: 'jsonp',
            jsonp: 'jsoncallback',
            data: { "loginname": $("#loginname").val(), "password": $("#password").val(), "Validate": $("#newvalidate").val(), "isautologin": i_},
            success: function(json){
                switch(json.status){
                    case "0":
                        $("#companyLoginregin").hide();
                        $("#companyLogining").show();
                        $("#personname").text(GetCookie("JSShowname"));
                        $("#loginNwid").hide();
                        break;
                    case "1":$(".errorstyle").hide();$("#error4").show();break;
                    case "2":$(".errorstyle").hide();$("#error3").show();break;
                    case "3":$(".errorstyle").hide();$("#error1").show();break;
                    case "4":$(".errorstyle").hide();$("#error2").show();break;
                    case "6":$(".errorstyle").hide();$("#error3").show();break;
                    case "7":$(".errorstyle").hide();$("#error3").show();break;
                    case "10":alert("账户未认证统一进入认证模块");break;
                    case "19":$(".errorstyle").hide();$("#error5").show();break;
                }
                var needpi = json.needpic ;
                if(needpi == "y"){
                    $("#yanzm").show();
                    document.getElementById('newvimg').src='http://my.zhaopin.com/myzhaopin/picturetimestamp.asp?t='+Date.parse(new Date());
                }
            },
            error:function(json){
               alert("登录错误，请稍后再试") ;
            }
        });
    });
}
</script> 
